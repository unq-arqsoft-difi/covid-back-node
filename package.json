{
  "name": "covid-back-node",
  "version": "1.0.0",
  "description": "covid-back-node",
  "main": "index.js",
  "author": "difi",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:production": "sequelize db:migrate && yarn start",
    "dev": "nodemon --ignore node_modules ./index.js",
    "lint": "eslint --ext .js ./ src/ test/",
    "test": "jest --verbose",
    "test:ci": "jest --verbose --coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint"
    }
  },
  "dependencies": {
    "@leandrojdl/express-routes": "^5.0.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "rm": "^0.1.8",
    "sequelize": "^5.21.12",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.4",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "collectCoverageFrom": [
      "src/*.js",
      "src/**/*.js"
    ]
  }
}
